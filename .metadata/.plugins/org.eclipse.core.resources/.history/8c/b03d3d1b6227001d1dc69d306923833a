
public class Exam_06 {
	public static void main(String[] args) {
		String name = "홍길동";
		System.out.println("안녕하세요 " + name + "님, 반갑습니다.");
		int a = 10;
		int b = 3;
		System.out.println("두수의 합: " + a + b);
		System.out.println("두수의 합: " + (a + b)); // 우선순위 적용
		
		System.out.println(a/b);
	}

}
