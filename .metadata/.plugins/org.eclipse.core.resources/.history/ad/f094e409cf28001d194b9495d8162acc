/*
 *    1    
 *   123   
 *  12345  
 * 1234567 
 *123456789
 * 1234567
 *  12345
 *   123
 *    1
6 1/4/3
7 2/3/2
8 3/2/1
9 4/1/0
 */


public class Exam_05 {
	public static void main(String[] args) {
		for(int i=1; i<=9; ++i) {
			if(i<=5) { // i = 1, 2, 3, 4, 5
				for(int k=1; k<=5-i; ++k) {
					System.out.print(" "); // 5-i개 만큼 공백
				}
				for(int j=1; j<=i+(i-1); ++j) { // i개에 i-1만큼 더 출력
					System.out.print(j);
				}
			}else { // i = 6, 7, 8, 9
				for(int u=1; u<=i-5; ++u) {
					System.out.print(" "); // i-5개 만큼 공백
				}
				for(int h=1; h<=(i)+(i+1); --h) { // i-2개에 i-1만큼 더 출력
					System.out.print(h);
				}
			}

			System.out.println(); // 개행
		}
	}
}