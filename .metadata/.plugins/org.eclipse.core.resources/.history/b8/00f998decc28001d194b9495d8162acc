/*
 *    1    
 *   123   
 *  12345  
 * 1234567 
 *123456789
 */
public class Exam_04 {
	public static void main(String[] args) {
		for(int i=1; i<=9; ++i) {
			for(int k=5; k>=i; --k) {
				System.out.print(" ");
			}
			
			for(int j=1; j<=i+2; j++) {
				System.out.print(j);
			}
			System.out.println();
		}
	}

}
