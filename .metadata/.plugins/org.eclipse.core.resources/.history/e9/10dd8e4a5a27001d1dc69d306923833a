
public class Exam_03 {
	public static void main(String[] args) {
		System.out.print("안녕하세요.");
		System.out.println("자바수업시간입니다.");
		System.out.println("수업시간은 9시 10분부터 5시 40분까지입니다.");
		System.out.printf("%6d + %.2f = %d\n", 12456, 20.0, 10+20);
		System.out.printf("%6d + %.2f = %d", 132, 20.0, 10+20);
	}
}
